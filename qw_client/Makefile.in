	$(MAKE_SURE_mgl_DIR)
	$(CC) $(CFLAGS) $(MGL_CFLAGS) -o $@ -c $<

$(BUILD_DIR)/mgl/%.o: $(SRC_DIR)/%.s
	$(MAKE_SURE_mgl_DIR)
	$(CC) $(CFLAGS) -x assembler-with-cpp -o $@ -c $<

$(BUILD_DIR)/mgl/%.o: $(COMMON_DIR)/%.c
	$(MAKE_SURE_mgl_DIR)
	$(CC) $(CFLAGS) $(MGL_CFLAGS) -o $@ -c $<

ifneq ($(QW_COMMON_DIR),)
$(BUILD_DIR)/mgl/%.o: $(QW_COMMON_DIR)/%.c
	$(MAKE_SURE_mgl_DIR)
	$(CC) $(CFLAGS) $(MGL_CFLAGS) -o $@ -c $<
endif

$(BUILD_DIR)/mgl/%.o: $(COMMON_DIR)/%.s
	$(MAKE_SURE_mgl_DIR)
	$(CC) $(CFLAGS) -x assembler-with-cpp -o $@ -c $<

$(MGLQUAKE): $(BUILD_DIR)/../$(MGLQUAKE)

$(BUILD_DIR)/../$(MGLQUAKE): $(OBJSquake-mgl)
	$(CC) $(CFLAGS) $(OBJSquake-mgl) $(MGL_LDFLAGS) $(LDFLAGS) -o $(BUILD_DIR)/../$(MGLQUAKE)
# XXX - Can this be automated?
clean-$(MGLQUAKE):
	rm -f $(OBJSquake-mgl)
